{
  "name": "nestjs-api-monitor",
  "version": "1.0.0",
  "description": "AI-Powered API Monitoring & Alert Bot as an NPM Library for NestJS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json && find dist -name \"*.spec.*\" -delete",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "publish:npm": "echo 'Publishing package...' && npm publish --access public || echo 'Publish failed, retrying...' && npm publish --access public --force",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "nestjs",
    "api",
    "monitoring",
    "alerts",
    "slack",
    "email",
    "twilio",
    "whatsapp",
    "openai",
    "error-handling"
  ],
  "author": "Anthony <your-email@example.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anthony/nestjs-api-monitor.git"
  },
  "bugs": {
    "url": "https://github.com/Anthony/nestjs-api-monitor/issues"
  },
  "homepage": "https://github.com/Anthony/nestjs-api-monitor#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "axios": "^1.6.0",
    "nestjs-slack-webhook": "^10.0.0",
    "nestjs-twilio": "^4.0.0",
    "nodemailer": "^6.9.0",
    "openai": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/nodemailer": "^6.4.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
