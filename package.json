{
  "name": "nestjs-api-monitor",
  "version": "1.0.0",
  "description": "AI-Powered API Monitoring & Alert Bot as an NPM Library for NestJS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "prepublish": "npm run build",
    "prepare": "npm run build"
  },
  "keywords": [
    "nestjs",
    "api",
    "monitoring",
    "alerts",
    "slack",
    "email",
    "twilio",
    "whatsapp",
    "openai",
    "error-handling"
  ],
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/nestjs-api-monitor"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "axios": "^1.6.0",
    "nestjs-slack-webhook": "^10.0.0",
    "nestjs-twilio": "^4.0.0",
    "nodemailer": "^6.9.0",
    "openai": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/nodemailer": "^6.4.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0",
    "eslint": "^8.40.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  }
}
